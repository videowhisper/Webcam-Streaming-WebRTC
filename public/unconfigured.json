{
    "deny": "Please fill in your WebRTC server details in this config file and remove/empty the deny property to continue. Get a free developer account from https://webrtchost.com/hosting-plans/#WebRTC-Only",
    "channel": "{generate}",
    "username": "{generate}",
    "view": "Broadcast",
    "enableGET": true,
    "showURL": true,
    "videowhisperServer":{
